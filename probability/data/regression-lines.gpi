# Preamble
load preload

# Labels
set key autotitle columnhead
set xlabel 'Input'
set ylabel 'Output'

# Plot
plot csv using 1:3 smooth unique with lines lc 'green' lw 4, \
     csv using 1:4 smooth unique with lines lc 'red' lw 4, \
     csv using 1:5 smooth unique with lines lc 'blue' lw 4, \
     csv using 1:2 with points ps 2 pointtype 7 lc 'black' title 'Sample Points', \
