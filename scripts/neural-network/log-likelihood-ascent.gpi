# Preamble
set loadpath load_path
set output "log-likelihood-ascent.png"

# Labels
set key autotitle columnhead
set xlabel 'Epochs'
set ylabel 'Log-Likelihood'
set logscale x

# Plot
plot "gradient-ascent.csv" using 0:1 with lines lc 'green' lw 4, \
     "gradient-ascent.csv" using 0:2 with lines lc 'red' lw 4, \
     "gradient-ascent.csv" using 0:3 with lines lc 'blue' lw 4, \
     "gradient-ascent.csv" using 0:4 with lines lc 'purple' lw 4 dashtype (20,20), \
