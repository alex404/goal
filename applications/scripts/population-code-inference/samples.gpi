# Preamble
load preload

# Labels
set xrange [0:2*pi]
set xtics ("0" 0,"0.5π" pi/2, "π" pi, "1.5π" 1.5*pi, "2π" 2*pi)
set xlabel '(Preferred) Stimulus'
set ylabel 'Spike Count'

# Plot
plot csv index 0 using 1:2 with points pt 7 lw 11 lc 'blue' title 'first', \
     csv index 0 using 1:3 with points pt 7 lw 8 lc 'red' title 'second', \
     csv index 0 using 1:4 with points pt 7 lw 3 lc 'green' title 'third', \
