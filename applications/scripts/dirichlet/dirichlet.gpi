# Preamble
load preload

# Settings
set key autotitle columnhead

stats csv index 0 nooutput

set xrange [0:1]
set yrange [0:1]

nlvls = STATS_records

plot for [i=1:nlvls] csv index i with lines smooth bezier notitle lc "black", \
    for [i=1:nlvls] csv index (i+1+nlvls) with lines smooth bezier notitle lc "red" dt (50,10), \
    csv index (2*nlvls+2) with points lc "black" notitle

