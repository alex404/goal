# Preamble
png_height = 800
png_width = 600
load preload

# Settings
set key autotitle columnhead
set boxwidth 0.8 relative
set style fill solid 0.5
titles = "Gains Means Precisions"

# Plot
set multiplot layout 3,1 title "Population Parameters"

do for [i = 0:2] {

    stats csv index i using 2 nooutput
    mxbn = STATS_max
    stp = ceil(mxbn/4) + 1
    set ytics stp nomirror
    set yrange [0:mxbn+stp]
    plot csv index i with boxes title word(titles,i+1) lc black

}

unset multiplot
