# Preamble
cd load_path
set output "conjugation-curves.png"
csv = "conjugation-curves.csv"

# Labels
set key autotitle columnhead
set xlabel 'Stimulus'
set ylabel 'Potential'
set xtics ("0" 0,"0.5π" pi/2, "π" pi, "1.5π" 1.5*pi, "2π" 2*pi)
set xrange [0:2*pi]


stats csv nooutput

# Plot
plot for [i=2:STATS_columns] csv using 1:i with lines lw 4
